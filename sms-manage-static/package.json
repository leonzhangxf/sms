{
  "name": "sms-manage-static",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "vue-cli-service serve --mode development --port 10001",
    "serve:test": "vue-cli-service serve --mode test --port 10002",
    "serve:prod": "vue-cli-service serve --mode production --port 10003",
    "build": "npm run build:prod",
    "build:dev": "vue-cli-service build --mode dev ",
    "build:test": "vue-cli-service build --mode test ",
    "build:prod": "vue-cli-service build --mode production ",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "iview": "^3.0.1",
    "js-cookie": "^2.2.0",
    "keycloak-js": "^4.4.0",
    "qs": "^6.5.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "webpack-dev-server": "^3.1.11",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-service": "^5.0.8",
    "vue-template-compiler": "^2.5.17",
    "sass-loader": "^7.1.0",
    "node-sass": "^4.9.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
